{
	"manifest_version": 2,

	"name": "School Shortcuts",
	"description": "These are some of my most use tabs in Y11 and are for private use",
	"version": "1.01",

	"browser_action": {
		"default_icon": "sources/myIcon16.png",
		"default_popup": "popup.html"
	},

	"icons": { 
		"16": "sources/myIcon16.png",
       	"48": "sources/myIcon48.png",
      	"128": "sources/myIcon128.png" 
    },

	"permissions": [
		"activeTab",
		"tabs"
	]
}